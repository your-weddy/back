<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.swyp.weddy.domain.wiki.dao.WikiMapper">

    <select id="selectWiki" parameterType="String" resultType="org.swyp.weddy.domain.wiki.domain.Wiki">
        SELECT title
        FROM wiki
        WHERE title = #{title}
    </select>

</mapper>
